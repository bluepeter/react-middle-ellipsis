{
  "name": "react-middle-ellipsis",
  "version": "1.0.5",
  "description": "Put the dots in the middle of a long string, versus the end.",
  "author": "bluepeter",
  "license": "MIT",
  "repository": "bluepeter/react-middle-ellipsis",
  "main": "dist/index.js",
  "module": "dist/index.min.js",
  "jsnext:main": "dist/index.min.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "lint": "eslint src/*js",
    "fix": "eslint --fix src/*js",
    "start": "yarn run dev",
    "prepare": "rm -rf ./dist/ && yarn run build",
    "predeploy": "cd ./example && yarn && yarn run build",
    "deploy": "gh-pages -d ./example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "gh-pages": "^2.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "^3.0.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "yargs": "^13.2.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
