{
  "name": "react-middle-ellipsis",
  "version": "1.2.1",
  "description": "Put the dots in the middle of a long string, versus the end.",
  "author": "bluepeter",
  "license": "MIT",
  "repository": "bluepeter/react-middle-ellipsis",
  "main": "dist/index.js",
  "module": "dist/index.min.js",
  "jsnext:main": "dist/index.min.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && webpack --env dev && webpack --env build",
    "dev": "rm -rf ./dist/ && webpack --progress --watch --env dev",
    "lint": "eslint src/*js",
    "fix": "eslint --fix src/*js",
    "start": "yarn run dev",
    "prepare": "yarn run build",
    "predeploy": "cd ./example && yarn && yarn run build",
    "deploy": "gh-pages -d ./example/build"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^7.26.0",
    "eslint-loader": "^4.0.2",
    "gh-pages": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "yargs": "^17.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
