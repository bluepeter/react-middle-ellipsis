{
  "name": "react-middle-ellipsis",
  "version": "1.3.0",
  "description": "Put the dots in the middle of a long string, versus the end.",
  "author": "bluepeter",
  "license": "MIT",
  "repository": "bluepeter/react-middle-ellipsis",
  "main": "dist/index.js",
  "module": "dist/index.min.js",
  "jsnext:main": "dist/index.min.js",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && webpack --env dev && webpack --env build",
    "dev": "rm -rf ./dist/ && webpack --progress --watch --env dev",
    "lint": "eslint src/*js",
    "fix": "eslint --fix src/*js",
    "start": "yarn run dev",
    "prepare": "yarn run build",
    "predeploy": "cd ./example && yarn && yarn run build",
    "deploy": "gh-pages -d ./example/build"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18 || ^19",
    "react-dom": "^16.8 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^8.57.0",
    "eslint-webpack-plugin": "^4.2.0",
    "gh-pages": "^6.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "webpack": "^5.97.0",
    "webpack-cli": "^6.0.1",
    "yargs": "^18.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "caniuse-lite": "^1.0.30001743"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
